{
  "$schema": "https://raw.githubusercontent.com/Septh/tmlanguage/master/tmlanguage.json",
  "name": "Glass",
  "scopeName": "source.glass-py",
  "patterns": [
    {
      "begin": "^---$",
      "beginCaptures": {
        "0": {
          "name": "string.other.begin.yaml"
        }
      },
      "end": "^---$",
      "endCaptures": {
        "0": {
          "name": "string.other.end.yaml"
        }
      },
      "contentName": "source.yaml",
      "patterns": [
        {
          "include": "source.yaml"
        }
      ]
    },
    {
      "include": "#transcript-element"
    },
    {
      "include": "#request-element"
    },
    {
      "include": "#test-element"
    },
    {
      "include": "#state-element"
    },
    {
      "include": "#user-element"
    },
    {
      "include": "#assistant-element"
    },
    {
      "include": "#system-element"
    },
    {
      "include": "#block-element"
    },
    {
      "include": "#for-element"
    },
    {
      "include": "source.python"
    }
  ],
  "repository": {
    "flow-expression": {
      "begin": "^(.*)(\\$\\{)",
      "end": "(\\})(.*)$",
      "beginCaptures": {
        "1": {
          "name": "text.html.markdown"
        },
        "2": {
          "name": "source.glass.flow.punctuation"
        }
      },
      "endCaptures": {
        "1": {
          "name": "source.glass.flow.punctuation"
        },
        "2": {
          "name": "text.html.markdown"
        }
      },
      "patterns": [
        {
          "include": "source.js"
        }
      ]
    },
    "for-element": {
      "begin": "(?<=^|\\n)[\\t ]*(<)(/?)(For)(?=[\\s\\/\\>\\{])",
      "beginCaptures": {
        "1": {
          "name": "punctuation.definition.tag.end.jsx"
        },
        "2": {
          "name": "punctuation.definition.tag.closing.jsx"
        },
        "3": {
          "name": "source.glass.for.name"
        }
      },
      "patterns": [
        {
          "include": "source.tsx#jsx-tag-attribute-name"
        },
        {
          "include": "source.tsx#jsx-tag-attribute-assignment"
        },
        {
          "include": "source.tsx#jsx-string-double-quoted"
        },
        {
          "include": "source.tsx#jsx-string-single-quoted"
        },
        {
          "include": "source.tsx#jsx-evaluated-code"
        }
      ],
      "end": "(?:(\\/)\\s*)?(>)$",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.self-closing.jsx"
        },
        "2": {
          "name": "punctuation.definition.tag.end.jsx"
        }
      }
    },
    "transcript-element": {
      "begin": "(?<=^|\\n)[\\t ]*(<)(/?)(Transcript)(?=[\\s\\/\\>\\{])",
      "beginCaptures": {
        "1": {
          "name": "punctuation.definition.tag.end.jsx"
        },
        "2": {
          "name": "punctuation.definition.tag.closing.jsx"
        },
        "3": {
          "name": "source.glass.transcript.name"
        }
      },
      "patterns": [
        {
          "include": "source.tsx#jsx-tag-attribute-name"
        },
        {
          "include": "source.tsx#jsx-tag-attribute-assignment"
        },
        {
          "include": "source.tsx#jsx-string-double-quoted"
        },
        {
          "include": "source.tsx#jsx-string-single-quoted"
        },
        {
          "include": "source.tsx#jsx-evaluated-code"
        }
      ],
      "end": "(?:(\\/)\\s*)?(>)$",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.self-closing.jsx"
        },
        "2": {
          "name": "punctuation.definition.tag.end.jsx"
        }
      }
    },
    "block-element": {
      "begin": "^(<)(Block)([^>]*)(>)$",
      "beginCaptures": {
        "1": {
          "name": "punctuation.definition.tag.end.jsx"
        },
        "2": {
          "name": "source.glass.block.name"
        },
        "3": {
          "patterns": [
            {
              "include": "source.tsx#jsx-tag-attribute-name"
            },
            {
              "include": "source.tsx#jsx-tag-attribute-assignment"
            },
            {
              "include": "source.tsx#jsx-string-double-quoted"
            },
            {
              "include": "source.tsx#jsx-string-single-quoted"
            },
            {
              "include": "source.tsx#jsx-evaluated-code"
            }
          ]
        },
        "4": {
          "name": "punctuation.definition.tag.end.jsx"
        }
      },
      "patterns": [
        {
          "include": "#flow-expression"
        },
        {
          "include": "#text-element"
        },
        {
          "include": "text.html.markdown"
        }
      ],
      "end": "^(<\\/)(Block)(>)$",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.end.jsx"
        },
        "2": {
          "name": "source.glass.block.name"
        },
        "3": {
          "name": "punctuation.definition.tag.end.jsx"
        }
      }
    },
    "system-element": {
      "begin": "^(<)(System)([^>]*)(>)$",
      "beginCaptures": {
        "1": {
          "name": "punctuation.definition.tag.end.jsx"
        },
        "2": {
          "name": "source.glass.system.name"
        },
        "3": {
          "patterns": [
            {
              "include": "source.tsx#jsx-tag-attribute-name"
            },
            {
              "include": "source.tsx#jsx-tag-attribute-assignment"
            },
            {
              "include": "source.tsx#jsx-string-double-quoted"
            },
            {
              "include": "source.tsx#jsx-string-single-quoted"
            },
            {
              "include": "source.tsx#jsx-evaluated-code"
            }
          ]
        },
        "4": {
          "name": "punctuation.definition.tag.end.jsx"
        }
      },
      "patterns": [
        {
          "include": "#flow-expression"
        },
        {
          "include": "#text-element"
        },
        {
          "include": "text.html.markdown"
        }
      ],
      "end": "^(<\\/)(System)(>)$",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.end.jsx"
        },
        "2": {
          "name": "source.glass.system.name"
        },
        "3": {
          "name": "punctuation.definition.tag.end.jsx"
        }
      }
    },
    "assistant-element": {
      "begin": "^(<)(Assistant)([^>]*)(>)$",
      "beginCaptures": {
        "1": {
          "name": "punctuation.definition.tag.end.jsx"
        },
        "2": {
          "name": "source.glass.assistant.name"
        },
        "3": {
          "patterns": [
            {
              "include": "source.tsx#jsx-tag-attribute-name"
            },
            {
              "include": "source.tsx#jsx-tag-attribute-assignment"
            },
            {
              "include": "source.tsx#jsx-string-double-quoted"
            },
            {
              "include": "source.tsx#jsx-string-single-quoted"
            },
            {
              "include": "source.tsx#jsx-evaluated-code"
            }
          ]
        },
        "4": {
          "name": "punctuation.definition.tag.end.jsx"
        }
      },
      "patterns": [
        {
          "include": "#flow-expression"
        },
        {
          "include": "#text-element"
        },
        {
          "include": "text.html.markdown"
        }
      ],
      "end": "^(<\\/)(Assistant)(>)$",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.end.jsx"
        },
        "2": {
          "name": "source.glass.assistant.name"
        },
        "3": {
          "name": "punctuation.definition.tag.end.jsx"
        }
      }
    },
    "state-element": {
      "begin": "^(<)(State)(>)$",
      "beginCaptures": {
        "1": {
          "name": "punctuation.definition.tag.end.jsx"
        },
        "2": {
          "name": "source.glass.state.name"
        },
        "3": {
          "name": "punctuation.definition.tag.end.jsx"
        }
      },
      "patterns": [
        {
          "include": "source.json"
        }
      ],
      "end": "^(<\\/)(State)(>)$",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.end.jsx"
        },
        "2": {
          "name": "source.glass.state.name"
        },
        "3": {
          "name": "punctuation.definition.tag.end.jsx"
        }
      }
    },
    "text-element": {
      "begin": "^(<)(Text)([^>]*)(>)$",
      "beginCaptures": {
        "1": {
          "name": "punctuation.definition.tag.end.jsx"
        },
        "2": {
          "name": "source.glass.text.name"
        },
        "3": {
          "patterns": [
            {
              "include": "source.tsx#jsx-tag-attribute-name"
            },
            {
              "include": "source.tsx#jsx-tag-attribute-assignment"
            },
            {
              "include": "source.tsx#jsx-string-double-quoted"
            },
            {
              "include": "source.tsx#jsx-string-single-quoted"
            },
            {
              "include": "source.tsx#jsx-evaluated-code"
            }
          ]
        },
        "4": {
          "name": "punctuation.definition.tag.end.jsx"
        }
      },
      "end": "^(<\\/)(Text)(>)$",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.end.jsx"
        },
        "2": {
          "name": "source.glass.text.name"
        },
        "3": {
          "name": "punctuation.definition.tag.end.jsx"
        }
      },
      "patterns": [
        {
          "include": "#flow-expression"
        },
        {
          "include": "text.html.markdown"
        }
      ]
    },
    "user-element": {
      "begin": "^(<)(User)([^>]*)(>)$",
      "beginCaptures": {
        "1": {
          "name": "punctuation.definition.tag.end.jsx"
        },
        "2": {
          "name": "source.glass.user.name"
        },
        "3": {
          "patterns": [
            {
              "include": "source.tsx#jsx-tag-attribute-name"
            },
            {
              "include": "source.tsx#jsx-tag-attribute-assignment"
            },
            {
              "include": "source.tsx#jsx-string-double-quoted"
            },
            {
              "include": "source.tsx#jsx-string-single-quoted"
            },
            {
              "include": "source.tsx#jsx-evaluated-code"
            }
          ]
        },
        "4": {
          "name": "punctuation.definition.tag.end.jsx"
        }
      },
      "end": "^(<\\/)(User)(>)$",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.end.jsx"
        },
        "2": {
          "name": "source.glass.user.name"
        },
        "3": {
          "name": "punctuation.definition.tag.end.jsx"
        }
      },
      "patterns": [
        {
          "include": "#flow-expression"
        },
        {
          "include": "#text-element"
        },
        {
          "include": "text.html.markdown"
        }
      ]
    },
    "line-comments": {
      "name": "source.glass.highlight",
      "match": "^//.*$"
    },
    "request-element": {
      "begin": "^(<)(Request)(?=[\\s\\/\\>\\{])",
      "beginCaptures": {
        "1": {
          "name": "punctuation.definition.tag.end.jsx"
        },
        "2": {
          "name": "source.glass.request.name"
        }
      },
      "patterns": [
        {
          "include": "source.tsx#jsx-tag-attribute-name"
        },
        {
          "include": "source.tsx#jsx-tag-attribute-assignment"
        },
        {
          "include": "source.tsx#jsx-string-double-quoted"
        },
        {
          "include": "source.tsx#jsx-string-single-quoted"
        },
        {
          "include": "source.tsx#jsx-evaluated-code"
        }
      ],
      "end": "(?:(\\/)\\s*)?(>)",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.self-closing.jsx"
        },
        "2": {
          "name": "punctuation.definition.tag.end.jsx"
        }
      }
    },
    "test-element": {
      "begin": "(<)(Test)(>)",
      "beginCaptures": {
        "1": {
          "name": "punctuation.definition.tag.end.jsx"
        },
        "2": {
          "name": "source.glass.test.name"
        },
        "3": {
          "name": "punctuation.definition.tag.end.jsx"
        }
      },
      "patterns": [
        {
          "include": "source.python"
        }
      ],
      "end": "(<\\/)(Test)(>)",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.end.jsx"
        },
        "2": {
          "name": "source.glass.test.name"
        },
        "3": {
          "name": "punctuation.definition.tag.end.jsx"
        }
      }
    }
  }
}
