{
  "name": "css-variables",
  "type": "css",
  "colors": {
    "editor.foreground": "#000001",
    "editor.background": "#000002",
    "terminal.ansiBlack": "#A00000",
    "terminal.ansiRed": "#A00001",
    "terminal.ansiGreen": "#A00002",
    "terminal.ansiYellow": "#A00003",
    "terminal.ansiBlue": "#A00004",
    "terminal.ansiMagenta": "#A00005",
    "terminal.ansiCyan": "#A00006",
    "terminal.ansiWhite": "#A00007",
    "terminal.ansiBrightBlack": "#A00008",
    "terminal.ansiBrightRed": "#A00009",
    "terminal.ansiBrightGreen": "#A00010",
    "terminal.ansiBrightYellow": "#A00011",
    "terminal.ansiBrightBlue": "#A00012",
    "terminal.ansiBrightMagenta": "#A00013",
    "terminal.ansiBrightCyan": "#A00014",
    "terminal.ansiBrightWhite": "#A00015"
  },
  "tokenColors": [
    {
      "scope": "source.glass.function.name",
      "settings": {
        "foreground": "#3EA295",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "source.glass.tool.name",
      "settings": {
        "foreground": "#67f49d",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "source.glass.request.name",
      "settings": {
        "foreground": "#3e69d1",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "source.glass.text.name",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "source.glass.block.name",
      "settings": {
        "foreground": "#9373DE",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "source.glass.state.name",
      "settings": {
        "foreground": "#7a7a7a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "source.glass.void",
      "settings": {
        "foreground": "#7a7a7a"
      }
    },
    {
      "scope": "source.glass.test.name",
      "settings": {
        "foreground": "#FFB454",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "source.glass.init.name",
      "settings": {
        "foreground": "#FFB454",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "source.glass.system.name",
      "settings": {
        "foreground": "#f467c6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "source.glass.for.name",
      "settings": {
        "foreground": "#E5C75E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "source.glass.user.name",
      "settings": {
        "foreground": "#5EC5E5",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "source.glass.assistant.name",
      "settings": {
        "foreground": "#4EC9B0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "source.glass.flow.punctuation",
      "settings": {
        "foreground": "#4dff00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.fenced_code.block.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
